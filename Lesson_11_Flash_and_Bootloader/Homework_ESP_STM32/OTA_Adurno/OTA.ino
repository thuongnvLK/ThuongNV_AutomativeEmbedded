void setup() {
  // Khởi tạo Serial Monitor với baud rate 115200
  Serial.begin(115200);
  while (!Serial) {
    ; // Chờ Serial Monitor kết nối
  }

  // Khởi động giao tiếp UART với STM32F1 ở baudrate 9600
  mySerial.begin(9600);

  Serial.println("System is ready.");
}

void loop() {
    // Kiểm tra nếu có dữ liệu mới từ UART
  if (mySerial.available()) {
    char receivedChar = mySerial.read();
    
    // Xử lý dữ liệu nhận được
    switch (receivedChar) {
      case '1':
        sendArray(dataToSend1, sizeof(dataToSend1));
        Serial.println("Sent data array 1.");
        break;
      case '2':
        sendArray(dataToSend2, sizeof(dataToSend2));
        Serial.println("Sent data array 2.");
        break;
      case '3':
        sendArray(dataToSend3, sizeof(dataToSend3));
        Serial.println("Sent data array 3.");
        break;
      default:
        Serial.println("Unknown command received.");
        break;
    }
  }
}

void sendArray(uint8_t* data, size_t length) {
  // Gửi dữ liệu byte-by-byte
  for (size_t i = 0; i < length; i++) {
    mySerial.write(data[i]);
    delay(5); // Thêm độ trễ nhỏ giữa các byte để đảm bảo dữ liệu được nhận đúng
  }
  // Gửi ký tự xuống dòng để báo hiệu kết thúc truyền dữ liệu
  mySerial.write('\r');
  mySerial.write('\n');
}
